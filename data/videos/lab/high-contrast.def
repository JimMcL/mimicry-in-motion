# YetAnotherTracker settings for extracting walking tracks from videos shot in the lab

# Scale can be measured from the board which has size 600mmx900mm
view-scale=?
view-rotation=90
fps=50
mask=true

blur-size=0
motion-detector=differences
foreground-segmenter=background-subtraction
background-method=none
threshold-method=global
threshold=60

dilation-erosion=6,-2
min-contour=30
max-contour=600
display-contours=true

csv=true
csv-comment-prefix=#

kalman=veryfast
max-jump=20
min-gap=10
first-tracking-frame=20
termination-border=20
display-tracks=true

