# YetAnotherTracker settings for extracting walking tracks from videos shot in the lab
# Used for small sample with uneven lighting.

# Scale can be measured from the board which has size 600mmx900mm
view-scale=?
view-rotation=90
mask=true
fps=240

blur-size=0
motion-detector=differences
foreground-segmenter=KNN:500:300:false
background-method=none

dilation-erosion=6,-2
min-contour=10
max-contour=200
display-contours=true

csv=false
csv-comment-prefix=#

kalman=veryfast
max-jump=20
min-gap=10
first-tracking-frame=60
termination-border=20
display-tracks=true

